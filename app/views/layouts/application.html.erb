<%= render "layouts/before" %>
  <%= render "layouts/before_content" %>

  <div class="col-md-8 col-lg-8">
    <%= yield %>
  </div>

  <div class="col-md-4 col-lg-4">
    <%= yield(:sidebar) %>
    <%= render 'layouts/ad_sidebar_first' %>
    <%= render_cell :sidebars, :recent_commodities %>
    <%= render 'layouts/ad_sidebar_second' %>
    <%= render_cell :sidebars, :recent_comments %>
    <%= render_cell :sidebars, :listen %>
  </div>
<%= render "layouts/after" %>
