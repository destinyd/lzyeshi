<%= simple_form_for(@group) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :category_list, input_html: { value: '夜市' } %>
    <%= f.input :name %>

    <%= f.input :price %>
    <%= f.input :reserve %>
    <%= f.input :text %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
